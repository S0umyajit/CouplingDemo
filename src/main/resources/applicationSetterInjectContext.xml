<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="specification" class="car.example.setter.injection.Specification">
        <property name="name" value="Soumya"/>
        <property name="models" value="Suzuki"/>
    </bean>

    <bean id="car" class="car.example.setter.injection.Car">
        <property name="specification" ref="specification"/>
    </bean>

</beans>